    <sectionGroup name="couchbase">
      <section name="epg" type="Couchbase.Configuration.CouchbaseClientSection, Couchbase" />
      <section name="mediamark" type="Couchbase.Configuration.CouchbaseClientSection, Couchbase" />
      <section name="statistics" type="Couchbase.Configuration.CouchbaseClientSection, Couchbase" />
      <section name="cache" type="Couchbase.Configuration.CouchbaseClientSection, Couchbase" />
      <section name="social" type="Couchbase.Configuration.CouchbaseClientSection, Couchbase" />
    </sectionGroup>
  </configSections>
  <TCMConfig URL="http://192.168.193.191:8080" Application="remotetasksservice" Host="local" Environment="main" AppID="5bf8cf60" AppSecret="5aaa99331c18f6bad4adeef93ab770c2" />
  <connectionStrings/>
  <couchbase>
    <epg>
      <servers bucket="epg_channels_schedule">
        <add uri="http://ott-rnd-1.dev.kaltura.com:8091/pools" />
