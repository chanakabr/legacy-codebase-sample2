<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="couchbase">
      <section name="socialhub" type="Couchbase.Configuration.CouchbaseClientSection, Couchbase" />
    </sectionGroup>
  </configSections>

  <couchbase>
    <socialhub>
      <servers bucket="SocialHub">
        <add uri="http://127.0.0.1:8091/pools" />
      </servers>
    </socialhub>
  </couchbase>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" /></startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-8.0.0.0" newVersion="8.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Enyim.Caching" publicKeyToken="05e9c6b5a9ec94c2" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.2.9.0" newVersion="1.2.9.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
