<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="couchbase">
      <section name="socialhub" type="Couchbase.Configuration.CouchbaseClientSection, Couchbase"/>
    </sectionGroup>
  </configSections>

  <couchbase>
    <socialhub>
      <servers bucket="SocialHub">
        <add uri="http://127.0.0.1:8091/pools"/>
      </servers>
    </socialhub>
  </couchbase>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
