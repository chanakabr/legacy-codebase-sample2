<%@ WebHandler Language="C#" Class="rsstv_ipvision_stb" %>

using System;
using System.Web;

public class rsstv_ipvision_stb : IHttpHandler {
    
    public void ProcessRequest (HttpContext context) {
        context.Server.Transfer(string.Concat(context.Request.RawUrl.Replace("_stb", ""), context.Request.RawUrl.Contains("?") ? "&dev=stb" : "?dev=stb"));
    }
 
    public bool IsReusable {
        get {
            return false;
        }
    }

}