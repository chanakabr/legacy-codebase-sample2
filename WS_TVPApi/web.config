<configuration>
  <configSections>
    <sectionGroup name="couchbaseClients">
      <section name="Couchbase" type="Couchbase.Configuration.Client.Providers.CouchbaseClientSection, Couchbase.NetClient" />
    </sectionGroup>
    <section name="TVinciDBConfig" type="TVPApi.TVinciDBConfiguration" allowLocation="true" allowDefinition="Everywhere" />
    <section name="CouchbaseSectionMapping" type="System.Configuration.NameValueFileSectionHandler,System, Version=1.0.3300.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
  </configSections>
  <couchbaseClients>
    <Couchbase useSsl="false">
      <!--<notification useSsl="false">-->
      <servers>
        <add uri="http://couchbase1:8091/pools" />
        <add uri="http://couchbase2:8091/pools" />
        <add uri="http://couchbase3:8091/pools" />
      </servers>
      <buckets>
        <add name="OTT_Apps" useSsl="false" password="" operationLifespan="20000">
          <connectionPool name="custom" maxSize="10" minSize="5" sendTimeout="12000"/>
        </add>
      </buckets>
    </Couchbase>
  </couchbaseClients>
  <CouchbaseSectionMapping>
    <add key="authorization" value="OTT_Apps" />
  </CouchbaseSectionMapping>
  <system.serviceModel>
    <bindings>
      <wsHttpBinding>
        <binding name="webHttpBindingCatalogBinding" openTimeout="00:10:00" receiveTimeout="00:10:00" sendTimeout="00:10:00" maxReceivedMessageSize="2147483647">
          <security mode="None" />
        </binding>
        <binding name="WSHttpBinding_Iservice" openTimeout="00:10:00" receiveTimeout="00:10:00" sendTimeout="00:10:00" maxReceivedMessageSize="2147483647">
          <security mode="None" />
        </binding>		
      </wsHttpBinding>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_ITVPConfigSvc" />
        <binding name="BasicHttpBinding_ITVPPageDataSvc" openTimeout="00:10:00" receiveTimeout="00:10:00" sendTimeout="00:10:00" maxReceivedMessageSize="2147483647">
        </binding>
		<binding name="basicHttpBinding" openTimeout="00:10:00" receiveTimeout="00:10:00" sendTimeout="00:10:00" maxReceivedMessageSize="2147483647"/>
		<binding name="BasicHttpBinding_IService" openTimeout="00:10:00" receiveTimeout="00:10:00" sendTimeout="00:10:00" maxReceivedMessageSize="2147483647"/>
      </basicHttpBinding>
    </bindings>
    <behaviors>
      <endpointBehaviors>
        <behavior name="web">
        </behavior>
      </endpointBehaviors>
      <serviceBehaviors>
        <behavior name="CatalogBehavior">
          <serviceMetadata httpGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="true" />
        </behavior>
        <!--<behavior name="">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="true"/> 
        </behavior>-->
      </serviceBehaviors>
    </behaviors>

    <client>
	      <endpoint address="http://msedge.prd.tvincidns.com/ws/pagedata/TVPPageDataSvc.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ITVPPageDataSvc" contract="PageDataSvc.ITVPPageDataSvc" behaviorConfiguration="web" name="BasicHttpBinding_ITVPPageDataSvc" />
      <endpoint address="http://msedge.prd.tvincidns.com/ws/TVPConfigSvc.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ITVPConfigSvc" contract="ConfigSvc.ITVPConfigSvc" name="BasicHttpBinding_ITVPConfigSvc" behaviorConfiguration="web"/>
      <endpoint address="http://webservices/catalog_v5_0_1/Service.svc" binding="wsHttpBinding" contract="TvinciPlatform.Catalog.Iservice" bindingConfiguration="webHttpBindingCatalogBinding" behaviorConfiguration="web" />
      <!-- <endpoint  binding="wsHttpBinding" contract="TvinciPlatform.Notification.INotificationService"
                  bindingConfiguration="webHttpBindingCatalogBinding" behaviorConfiguration="web"/> -->
      <endpoint binding="basicHttpBinding" behaviorConfiguration="web" contract="TvinciPlatform.Notification.INotificationService" bindingConfiguration="BasicHttpBinding_ITVPConfigSvc" />
    </client>

  </system.serviceModel>
  <TVinciDBConfig User="production" Pass="lF6CZU9HIOIAGuzj" DBServer="amazonSQL" DBInstance="TVinci"></TVinciDBConfig>
  <appSettings file="Configuration/GlobalAppSettings.config">
    <add key="AppEnvironment" value="test" />
    <add key="TVPPro.Configuration.Online" value="~/Configuration/OnlineConfiguration.config" />
    <add key="Log4NetConfiguration" value="log4net.config" />
    <add key="log4net.Internal.Debug" value="false" />
    <add key="TechnicalIP" value="*" />
    <add key="IsDebug" value="false" />
    <add key="Tvinci.DataLoader.CacheLite.ShouldUseCache" value="true" />
    <add key="il.co.smscenter.www.SendMessage" value="http://www.smscenter.co.il/Web/WebServices/SendMessage.asmx" />
    <add key="ShouldUseNewCache" value="true" />
    <add key="UseAlwaysOn" value="true" />

    <!--222_Web,222_Cellular,222_iPad,222_STB -->

    <!-- <add key="Authorization.GroupConfigsTtlSeconds" value="120"/> -->
    <!--<add key="Authorization.UnsupportedGroupsPlatforms" value="{groupID}_{Platform}"/>-->
    <add key="Authorization.UnsupportedGroupsPlatforms" value="306_Web,306_iPad,306_Cellular,225_Web,225_Cellular,225_iPad,222_Web,222_Cellular,222_iPad,222_STB,219_Web,219_Cellular,219_iPad,219_STB,219_ConnectedTV,214_Web,214_Cellular,214_iPad,214_STB,198_Web,198_STB,198_iPad,198_Cellular,185_Cellular,185_STB,185_iPad,195_Web,195_Cellular,195_iPad,195_STB,167_Web,167_Cellular,167_iPad,167_STB,159_Web,159_Cellular,159_iPad,159_STB,203_Web"/>
