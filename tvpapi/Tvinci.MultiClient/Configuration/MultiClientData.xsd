<xs:schema xmlns:mstns="http://tempuri.org/MultiClientConfiguration.xsd" elementFormDefault="qualified"  id="MultiClientConfiguration" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="MultiClientData">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Clients">
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="0" maxOccurs="unbounded" name="Client">
                <xs:complexType>
                  <xs:attribute name="ClientID" type="xs:string" use="required" />
                  <xs:attribute name="ConfigurationID" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Definitions">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Defaults">
                <xs:complexType>
                  <xs:attribute name="ClientID" type="xs:string" />
