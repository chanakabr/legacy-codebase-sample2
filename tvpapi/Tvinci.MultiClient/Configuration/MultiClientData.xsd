                  <xs:sequence>
                    <xs:element minOccurs="0" maxOccurs="unbounded" name="IgnoredPage" type="xs:string" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
