                </xs:restriction>
              </xs:simpleType>
            </xs:attribute>
            <xs:attribute default="true" name="ClientPersistByCookie" type="xs:boolean" />
            <xs:attribute default="true" name="ShowExitButton" type="xs:boolean" />
          </xs:complexType>
        </xs:element>
        <xs:element name="Pages">
