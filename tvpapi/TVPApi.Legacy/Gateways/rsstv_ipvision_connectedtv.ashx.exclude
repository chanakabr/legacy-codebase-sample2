<%@ WebHandler Language="C#" Class="rsstv_ipvision_connectedtv" %>

using System;
using System.Web;

public class rsstv_ipvision_connectedtv : IHttpHandler {

    public void ProcessRequest(HttpContext context)
    {
        context.Server.Transfer(string.Concat(context.Request.RawUrl.Replace("_connectedtv", ""), context.Request.RawUrl.Contains("?") ? "&dev=connectedtv" : "?dev=connectedtv"));
    }
 
    public bool IsReusable {
        get {
            return false;
        }
    }

}