						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="Tag">
		<xs:attribute name="Type" type="xs:int"/>
		<xs:attribute name="ID" type="xs:int"/>
		<xs:attribute name="Value" type="xs:string"/>
