        "type": "object",
        "description": "Filtering Asset Struct Metas",
        "allOf": [
          {
            "$ref": "ott-schema-registry/models/api/rest/kalturaFilter",
            "x-template-instantiation": {
              "KalturaT": "#/components/schemas/KalturaAssetFilePpvOrderBy"
            }
          },
          {
            "properties": {
              "assetIdEqual": {
                "type": "integer",
                "format": "int64",
                "description": "Filter Asset file ppvs that contain a specific asset id",
                "nullable": true,
                "minimum": 1
              },
              "assetFileIdEqual": {
                "type": "integer",
                "format": "int64",
                "description": "Filter Asset file ppvs that contain a specific asset file id",
                "nullable": true,
                "minimum": 1
              },
              "orderBy": {
                "default": "NONE",
                "allOf": [
                  {
                    "$ref": "#/components/schemas/KalturaAssetFilePpvOrderBy"
                  }
                ],
                "x-override-default": true,
                "x-get-default-orderby": true
              }
            }
          }
        ],
        "x-namespace": "WebAPI.Models.Pricing"
      },
