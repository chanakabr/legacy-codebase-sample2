        <binding name="BasicHttpBinding_IService"/>
        <binding name="BasicHttpBinding_INotificationService" openTimeout="00:10:00" receiveTimeout="00:10:00" sendTimeout="00:10:00" maxReceivedMessageSize="2147483647">
          <security mode="None"/>
        </binding>
